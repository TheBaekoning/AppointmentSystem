<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <TextField fx:id="loginUser" layoutX="46.0" layoutY="114.0" />
      <PasswordField fx:id="loginPassword" layoutX="320.0" layoutY="114.0" />
      <Text fx:id="userText" layoutX="46.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0" text="User Name" />
      <Text fx:id="passwordText" layoutX="320.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
      <Button fx:id="loginButton" layoutX="46.0" layoutY="194.0" mnemonicParsing="false" onAction="#loginValidation" text="Login" />
   </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="488.0" prefWidth="1040.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Customer">
   <children>
      <Button fx:id="addCustomerButton" layoutX="65.0" layoutY="318.0" mnemonicParsing="false" onAction="#addCustomerClicked" text="Add New Customer" />
      <Button fx:id="updateCustomerButton" layoutX="240.0" layoutY="318.0" mnemonicParsing="false" onAction="#updateCustomerClicked" text="Update Customer" />
      <Button fx:id="cancelButton" layoutX="942.0" layoutY="422.0" mnemonicParsing="false" onAction="#cancelButtonClicked" text="Cancel" />
      <Button fx:id="deleteCustomerButton" layoutX="868.0" layoutY="318.0" mnemonicParsing="false" onAction="#deleteCustomerClicked" text="Delete Customer" />
      <TableView fx:id="customerTable" layoutX="65.0" layoutY="49.0" prefHeight="239.0" prefWidth="931.0">
        <columns>
          <TableColumn fx:id="customerName" prefWidth="193.0" text="Customer Name" />
          <TableColumn fx:id="customerAddress" minWidth="0.0" prefWidth="415.0" text="Customer Address" />
            <TableColumn fx:id="customerPhone" minWidth="0.0" prefWidth="183.0" text="Customer Phone" />
        </columns>
      </TableView>
   </children>
</AnchorPane>

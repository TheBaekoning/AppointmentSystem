<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UpdateAppointment">
   <children>
      <TextField fx:id="startBox" layoutX="38.0" layoutY="68.0" />
      <TextField fx:id="endBox" layoutX="300.0" layoutY="68.0" />
      <TextField fx:id="typeBox" layoutX="38.0" layoutY="150.0" />
      <Text layoutX="38.0" layoutY="51.0" strokeType="OUTSIDE" strokeWidth="0.0" text="New Start" />
      <Text layoutX="300.0" layoutY="51.0" strokeType="OUTSIDE" strokeWidth="0.0" text="New End" />
      <Text layoutX="38.0" layoutY="135.0" strokeType="OUTSIDE" strokeWidth="0.0" text="New Type" />
      <Button fx:id="updateButton" layoutX="38.0" layoutY="329.0" mnemonicParsing="false" onAction="#onClickUpdate" text="Update Appointment" />
      <Button fx:id="cancelButton" layoutX="505.0" layoutY="329.0" mnemonicParsing="false" onAction="#cancelButtonClicked" text="Cancel" />
   </children>
</AnchorPane>
